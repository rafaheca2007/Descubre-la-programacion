int x= 160;
int y= 160;

void main () {
  animate(ejecutar);
  
}
void ejecutar (){
  player();
  controles();
}

void player(){
  
background(0,0,0);
  ellipse(x,y,30,30);
}
void controles(){
  if(keyPressed && key =="up"){
    print("hoola");
    y = y -1;
  }
     
     }

void draw(){
  background(0, 0, 0);
  ellipse (x,y, 30, 30);
	if (x < -130);
  	x = 0;
  if (x > 130);
  	x = 0;
  if (y < 130);
  	x = 0;
  if (y > 130);
  	x = 0;
  
  }

  
